{
    "servers": {
        "odin": {
            "command": [
                "/home/moena/odin/ols/ols"
            ],
            "filetypes": [
                "odin"
            ],
            "url": "https://github.com/DanielGavin/ols",
            "root": "%{Project:NativePath}",
            "highlightingModeRegex": "^Odin$",
            "initializationOptions": {
                "collections": [
                    {
                        "name": "core",
                        "path": "/home/moena/odin/core"
                    },
                    {
                        "name": "vendor",
                        "path": "/home/moena/odin/vendor"
                    },
                    {
                        "name": "shared",
                        "path": "/home/moena/odin/shared"
                    }
                ],
                "odin_command": "/home/moena/odin/odin",
                "verbose": true,
                "enable_document_symbols": true,
                "enable_hover": true
            }
        }
    }
}
